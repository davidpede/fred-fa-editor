{
  "name": "FredFAEditor",
  "lowCaseName": "fredfaeditor",
  "description": "FontAwesome Editor for Fred",
  "author": "John Peca",
  "version": "1.0.0-rc",
  "package": {
    "menus": [],
    "elements": {
      "plugins": [
        {
          "name": "FredFAEditor",
          "file": "FredFAEditor.php",
          "events": [
            "FredBeforeRender"
          ]
        }
      ]
    }
  },
  "dependencies": [
    {
      "name": "fred",
      "version": ">=1.0.0"
    }
  ],
  "build": {
    "resolver": {
      "after": [
        "resolve.system_setting.php"
      ]
    }
  }
}
